pipeline {
  agent any
  stages {
    stage("code") {
	    steps{
		 git url: "https://github.com/Yatingambhir85/jenkins-demo-task.git", branch: main
	    }    
    stage('Build') {
      steps {
	echo "Building docker image"
        sh 'docker build -t my-image .'
      }
    }
   
  }
}
}
